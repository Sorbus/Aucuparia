<%= form_for @user_session, url: user_sessions_path do |f| %>
	<p>
		<%= f.label :email %>
		<%= f.text_field :email %>
	</p>
	
	<p>
		<%= f.label :password %>
		<%= f.password_field :password %>
	</p>
	
	<p>
		<%= f.label :remember_me %>
		<%= f.check_box :remember_me %>
	</p>
	<p>
		<%= f.submit "Login" %>
	</p>
<% end %>
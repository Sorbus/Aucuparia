<div class="content menu_edit narrow">
	<%= render 'admin/admin/bar' %>
	<p class="definition">
		<%= (t :menu_definition).html_safe %>
	</p>
	<%= @menus.each do |menu| %>
		<%= render 'form', :menu => menu, :path => admin_menu_path(menu)%>
	<% end %>
	<%= render 'form', :menu => @menu, :path => admin_menus_path %>
</div>